<div id="acousticness">
  <label for="acousticness" class="form-label">Acousticness</label>
  <input type="range" class="form-range" min="0" max="1" step="0.0001" value=<%= @track.audio_features.acousticness %> id="acousticness">
  <%= raise %>
</div>

<div id="danceability">
  <label for="danceability" class="form-label">Danceability</label>
  <input type="range" class="form-range" min="0" max="1" step="0.0001" value=<%= @track.audio_features.danceability %> id="danceability">
</div>

<div id="energy">
  <label for="energy" class="form-label">Energy</label>
  <input type="range" class="form-range" min="0" max="1" step="0.0001" value=<%= @track.audio_features.energy %> id="energy">
</div>

<div id="instrumentalness">
  <label for="instrumentalness" class="form-label">Instrumentalness</label>
  <input type="range" class="form-range" min="0" max="1" step="0.0001" value=<%= @track.audio_features.instrumentalness %> id="instrumentalness">
</div>

<div id="liveness">
  <label for="liveness" class="form-label">Liveness</label>
  <input type="range" class="form-range" min="0" max="1" step="0.0001" value=<%= @track.audio_features.liveness %> id="liveness">
</div>

<div id="loudness">
  <label for="loudness" class="form-label">Loudness</label>
  <input type="range" class="form-range" min="-60" max="0" step="0.006" value=<%= @track.audio_features.loudness %> id="loudness">
</div>

<div id="speechiness">
  <label for="speechiness" class="form-label">Speechiness</label>
  <input type="range" class="form-range" min="0" max="1" step="0.0001" value=<%= @track.audio_features.speechiness %> id="speechiness">
</div>

<div id="valence">
  <label for="valence" class="form-label">Valence</label>
  <input type="range" class="form-range" min="0" max="1" step="0.0001" value=<%= @track.audio_features.valence %> id="valence">
</div>

<div id="popularity">
  <label for="popularity" class="form-label">Popularity</label>
  <input type="range" class="form-range" min="0" max="100" step="0.01" value=<%= @track.popularity %> id="popularity">
</div>
